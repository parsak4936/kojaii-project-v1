ound-size: contain;\r\n        background-repeat: no-repeat;\r\n        \r\n\r\n        transition: all 0.3s ease-out;\r\n\r\n\r\n    }\r\n\r\n    /* interduceing section*/\r\n    .interduce-container {\r\n        width: 100%;\r\n        height: 660px;\r\n        display: grid;\r\n        justify-content: center;\r\n        grid-template-columns: 1fr  ;\r\n        grid-template-rows: .2fr .3fr .4fr;\r\n        grid-template-areas: \" header\" \"  text\" \"img\";\r\n    }\r\n     \r\n\r\n    .interduce-text {\r\n        margin-left: 50px;\r\n        margin-right: 50px;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n\r\n    }\r\n\r\n  \r\n\r\n    /* joing us section*/\r\n    .joingus-container {\r\n        width: 100%;\r\n        height: 630px;\r\n        display: grid;\r\n        grid-template-columns: 1fr  ;\r\n        justify-content: center;\r\n        grid-template-rows: .4fr .4fr .4fr;\r\n        grid-template-areas:    \" joingus_img  \"  \"joinus_header   \" \"joinus_txt    \";\r\n    }\r\n    .joinus-text{\r\n        margin-left: 50px;\r\n        margin-right: 50px;\r\n    }\r\n    \r\n}\r\n\r\n\r\n@media only screen and (max-width: 300px) {\r\n.where-are-u{\r\n    font-size: 35px;\r\n}\r\n\r\n.downlload-btn-mainpage-2-mobile {\r\n    \r\n    \r\n   margin-left: 25px;\r\n \r\n}\r\n.downlload-btn-mainpage-1-mobile {\r\n    \r\n  \r\n   \r\n  margin-left: 25px;\r\n\r\n  \r\n\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n}\r\n
