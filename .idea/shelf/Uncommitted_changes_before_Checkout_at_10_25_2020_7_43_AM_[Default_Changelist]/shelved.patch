<+>\r\n\r\n:root {\r\n    --main-radius: 5px;\r\n    --main-padding: 5px;\r\n}\r\n/*  Desktop */\r\n.mainpage-container {\r\n    display: grid;\r\n\r\n    grid-template-columns: 1fr;\r\n    grid-template-rows: 0fr 0fr 0fr 0fr;\r\n    grid-template-areas: \"main_img\" \"interduce\" \"Toturial\" \"join_us\";\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n\r\n}\r\n\r\n.toturial {\r\n    background-color: cyan;\r\n    grid-area: Toturial;\r\n  \r\n    justify-content: center;\r\n    display: flex;\r\n}\r\n\r\n.image_and_download {\r\n    background-image: url('./MP_Backgound.jpg');\r\n     background-repeat: no-repeat;\r\n    grid-area: main_img;\r\n    background-position: center; \r\n    background-size: cover;\r\n     display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n\r\n}\r\n\r\n.img_and_download-container {\r\n    width: 100%;\r\n    height: 620px;\r\n    display: grid;\r\n justify-content: center;\r\n     grid-template-columns:  1fr    1fr     ;\r\n    grid-template-rows: .3fr 0.2fr .2fr;\r\n    grid-template-areas: \"IAD_header  IAD_header\" \"IAD_txt   IAD_txt \" \"btn_bazar   btn_googleplay  \";\r\n}\r\n.blue-kojaii{\r\n    grid-area: IAD_kojaii;\r\n    color: #7ab1ff;\r\n    padding-right: 10px;\r\n}\r\n\r\n.come-faster {\r\n     grid-area: IAD_txt;\r\n     font: 35px bold;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    \r\n    \r\n   \r\n    color: white;\r\n}\r\n\r\n.where-are-u {\r\n\r\n    font: 45px bold;\r\n    grid-area: IAD_header;\r\n    display: flex;\r\n    margin-top: 50px;\r\n    justify-content: center;\r\n    color: white;\r\n}\r\n\r\n.downlload-btn-mainpage-1 {\r\n\r\n    grid-area: btn_bazar;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n\r\n.downlload-btn-mainpage-2 {\r\n\r\n\r\n    grid-area: btn_googleplay;\r\n\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.downlload-btn-mainpage-1-mobile {\r\n    display: none;\r\n}\r\n\r\n.downlload-btn-mainpage-2-mobile {\r\n    display: none;\r\n}\r\n\r\n\r\n/* Interduce section */\r\n.interduce {\r\n    background-color: black;\r\n    grid-area: interduce;\r\n}\r\n\r\n.interduce-container {\r\n    width: 100%;\r\n    height: 620px;\r\n    display: grid;\r\n    justify-content: center;\r\n    grid-template-columns: .4fr .4fr;\r\n    grid-template-rows: 1fr 1fr;\r\n    grid-template-areas: \"img header\" \"text text\";\r\n}\r\n\r\n.interduce-header {\r\n    grid-area: header;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n}\r\n\r\n.interduce-text {\r\n    grid-area: text;\r\n    display: flex;\r\n\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n}\r\n\r\n\r\n.animation-container {\r\n    grid-area: img;\r\n    align-items: center;\r\n    justify-content: center;\r\n    display: flex;\r\n\r\n\r\n}\r\n\r\n\r\n/* join us section */\r\n.join-us {\r\n    align-items: center;\r\n    justify-content: center;\r\n    grid-area: join_us;\r\n}\r\n\r\n.joingus-container {\r\n    width: 100%;\r\n    height: 550px;\r\n    justify-content: center;\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n    grid-template-rows: 1fr 1fr 1fr;\r\n    grid-template-areas: \"joinus_header  joingus_img\" \"joinus_txt  joingus_img \" \"joingus_btn  joingus_img  \";\r\n}\r\n\r\n.joinus-header {\r\n    grid-area: joinus_header;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n\r\n}\r\n\r\n.joinus-text {\r\n    grid-area: joinus_txt;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n\r\n}\r\n\r\n.join-us-img-container {\r\n    font-family: \"Iranian Sans\" !important;\r\n    grid-area: joingus_img;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n}\r\n\r\n.join-us-form{\r\n    direction: rtl;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.join-us-btn{\r\n    margin-top: 20px !important;\r\n    margin-bottom: 20px !important;\r\n}\r\n\r\n\r\n.join-us-param{\r\n    border: 1px solid #343a40;\r\n    border-radius: 5px;\r\n    margin-left: 20px;\r\n    padding: 10px;\r\n}\r\n\r\n/*  About us section */\r\n.About-us {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n    grid-area: \"AboutUs\";\r\n    background-color: coral;\r\n}\r\n\r\n.info {\r\n\r\n\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    grid-template-rows: 1fr 1fr;\r\n    grid-template-areas: \"text_ text_ text_\";\r\n    align-items: center;\r\n    justify-content: center;\r\n\r\n\r\n}\r\n\r\n.aboutus-txt {\r\n    flex-direction: column;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    padding: 15px;\r\n}\r\n\r\n\r\n\r\n/*  Tablet */\r\n\r\n\r\n@media only screen and (max-width: 900px) {\r\n    .mainpage-container {\r\n        justify-content: center;\r\n        grid-template-columns: 1fr;\r\n        grid-template-rows: 0fr 0fr 0fr 0fr;\r\n        grid-template-areas: \"main_img\" \"interduce\" \"Toturial\" \"join_us\";\r\n    }\r\n\r\n    .toturial {\r\n\r\n        grid-area: Toturial;\r\n        width: \"100%\",;\r\n        height: \"600px\";\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n    }\r\n\r\n    \r\n\r\n    /* Image and download section */\r\n    .img_and_download-container {\r\n        width: 100%;\r\n  \r\n      height: 590px;\r\n        display: grid;\r\n       \r\n        justify-content: center;\r\n        grid-template-columns: .5fr      ;\r\n        grid-template-rows: .2fr 0.2fr 0.1fr 0fr ;\r\n        grid-template-areas: \"IAD_header    \" \"  IAD_txt    \" \"btn_bazar   \"   \"  btn_googleplay \";\r\n    }\r\n    \r\n     \r\n  \r\n\r\n    /* Set text color to red */\r\n    .downlload-btn-mainpage-1-mobile {\r\n       \r\n        display: block;\r\n        text-align: center;\r\n        align-items: center;\r\n        justify-content: center;\r\n        border-radius: 4px;\r\n        margin-bottom: 30px;\r\n        border: none;\r\n        background: url('./Bazar.png');\r\n        background-size: contain;\r\n        background-repeat: no-repeat;\r\n       \r\n\r\n        transition: all 0.3s ease-out;\r\n\r\n\r\n    }\r\n    .where-are-u{\r\n        font-size: 50px;\r\n    }\r\n    .come-faster{\r\n        font-size: 30px;\r\n    \r\n    }\r\n \r\n    .downlload-btn-mainpage-2-mobile {\r\n        align-items: center;\r\n        justify-content: center;\r\n        display: block;\r\n        text-align: center;\r\n        margin-bottom: 30px;\r\n        border: none;\r\n        background: url('./Bazar.png');\r\n        background-size: contain;\r\n        background-repeat: no-repeat;\r\n        transition: all 0.3s ease-out;\r\n\r\n    }\r\n\r\n    /* interduceing section*/\r\n    .interduce-container {\r\n        align-items: center;\r\n        justify-content: center;\r\n        width: 100%;\r\n        height: 660px;\r\n        display: grid;\r\n        grid-template-columns: .8fr  ;\r\n        grid-template-rows: 0.1fr .4fr .4fr;\r\n        grid-template-areas: \" header\" \"  text\" \"img\";\r\n    }\r\n    \r\n.interduce-header{\r\n    margin-top: 60px;\r\n}\r\n    .interduce-text {\r\n        margin-left: 80px;\r\n        margin-right: 80px;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n\r\n    }\r\n\r\n    \r\n\r\n    /* joing us section*/\r\n    .joingus-container {\r\n        width: 100%;\r\n        height: 660px;\r\n        display: grid;\r\n         justify-content: center;\r\n        grid-template-columns: 1fr  ;\r\n        grid-template-rows: .4fr .4fr .4fr;\r\n        grid-template-areas: \"joinus_header   \" \"joinus_txt    \"   \" joingus_img  \";\r\n    }\r\n     \r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n/*  Phone */\