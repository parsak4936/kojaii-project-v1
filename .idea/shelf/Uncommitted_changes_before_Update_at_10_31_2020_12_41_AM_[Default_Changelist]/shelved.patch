r\n\r\n                            <img className=\"join-us-image\" name=\"joinus-img\" src={join_Us_img}/>\r\n\r\n                        </div>\r\n\r\n\r\n                    </div>\r\n\r\n\r\n                </section>\r\n\r\n\r\n\r\n            </div>\r\n        </>\r\n    );\r\n\r\n\r\n}\r\n\r\nexport default MainPage;
